{
  "name": "playwright-phptravels",
  "version": "1.0.0",
  "description": "Playwright Retreats tests",
  "main": "index.js",
  "scripts": {
    "kill:processes": "./scripts/kill_processes.sh",
    "kill:ports": "./scripts/kill_ports.sh",
    "kill:npm": "pkill -f 'npm run'",
    "setup": "npm i && npm i -g ts-node && npx playwright install --with-deps",
    "ui:mode:prod": "cross-env test_env=prod npx playwright test --ui --project=phptravels:e2e",
    "template:prod": "clear && node run-playwright.js chrome template prod",
    "pretify": "npx prettier --write './APIs' './Pages' './TestData' './tests'",
    "chat:e2e:tests:prod": "clear && node run-playwright.js chrome chat:e2e prod",
    "softserve:bullies:tests:prod": "clear && node run-playwright.js chrome softserve:bullies prod",
    "show:report": "npx playwright show-report html_report"
  },
  "types": "./global.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexzavg/playwright_retreat"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-crypto/crc32": "^5.2.0",
    "@playwright/test": "^1.51.1",
    "@types/xml2js": "^0.4.14",
    "aws-sdk": "^2.1692.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "playwright": "^1.51.1"
  }
}
